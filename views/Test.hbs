<html>
<head id="init">
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0"> 
	<title>{{{title}}}</title>

	<link href="css/style.css" rel="stylesheet">
	<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">             
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
	<script src="https://kit.fontawesome.com/54e64f34b2.js" crossorigin="anonymous"></script>
	<script src="https://cdn.rawgit.com/hilios/jQuery.countdown/2.2.0/dist/jquery.countdown.min.js"></script>
		<!--[if lt IE 9]>
		  <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
		  <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
		<![endif]-->
	</head>
	<body> 

		<div class="preloader-wrapper">
			<div class="preloader">
				<img src="images/loader.gif" alt="Loading..." width="100px">
			</div>
		</div>


		<header>
			<div class="container">
				<div class="row">
					<div class="col s3">
						<a href="#" class="logo">
							<img src="images/logo.png" alt="Универмаг Московский">
						</a>
					</div>

					<div class="col s9">
						<ul class="nav">
							<li>
								<a class="modal-trigger" href="#modal1">
									История
								</a>
							</li>
							<li class="withDropdown">
								<a href="#">
									Адаптация
									<i class="fas fa-chevron-down"></i>
								</a>
								<ul>
									<li><a href="#">Lorem ipsum.</a></li>
									<li><a href="#">Dolor, quasi!</a></li>
									<li><a href="#">Obcaecati, sit.</a></li>
								</ul>
							</li>
							<li class="withDropdown">
								<a href="#">
									Аттестация
									<i class="fas fa-chevron-down"></i>
								</a>
								<ul>
									<li><a href="#">Lorem ipsum.</a></li>
									<li><a href="#">Dolor, quasi!</a></li>
									<li><a href="#">Obcaecati, sit.</a></li>
								</ul>
							</li>
							<li>
								<a href="#">
									Опросы
								</a>
							</li>
							<li>
								<a href="#">
									Связь
								</a>
							</li>
							<li class="withSep">
								<a href="#">
									{{{user_name}}}
								</a>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</header>


		<div class="wrapper quiz">


			<div class="container">
				<div class="row">
					<div class="col s3">
						<div class="study_guide">
							<ul class="quiz_list"> 
								{{#each tests_list}}
								<li>
									<a href="{{this.link}}">
										{{this.name}}
									</a>
								</li>
								{{/each}}
							</ul>
						</div>
					</div>

					<div class="col s9">
						<div class="study_content test">
							<div class="test_instrution">
								<h1 class="tac">
									{{{choosen_test.name}}}
								</h1>
								<hr>
								<div class="test_i_text">
									Ограничение по времени - 60 минут
								</div>
								<div class="tac">
									<button class="huge_button" id="start_test">
										Начать
									</button>
								</div>
							</div>

							<div class="test_body" style="display: none;">
								
								<div class="countdown">
									<span id="clock"></span>
								</div>

								{{#each choosen_test.tasks}}
								{{#if this.type == 'select'}}
								<div class="quiz_item">
									<div class="qi_title">
										{{this.question}}
									</div>
									<form action="#">
										{{#each this.variants}}
										<p>
											<label>
												<input name="group1" type="radio" checked>
												<span>{{this.var_name}}</span>
											</label>
										</p>
										{{/each}}
									</form>
								</div>
								{{/if}}	

								{{#if this.type == 'input'}}
								<div class="quiz_item">
									<div class="qi_title">
										{{this.question}}
									</div>
									<div class="input-field col s6">
										<input placeholder="Ответ" id="first_name" type="text" class="validate">
									</div>
								</div>
								{{/if}}	
								{{/each}}

							</div>
						</div>
					</div>
				</div>  
			</div> 


		</div>


		<footer>
			<div class="container">
				<div class="row">
					<div class="col s3">
						<a href="#" class="logo">
							<img src="images/logo.png" alt="Универмаг Московский">
						</a>
					</div>
					<div class="col s9">
						<ul class="nav">
							<li>
								<a href="#">
									Задать вопрос
								</a>
							</li>
							<li>
								<a href="#">
									Контакты
								</a>
							</li>
							<li>
								<a href="#">
									Сообщить об ошибке
								</a>
							</li>
							<li class="withSep">
								<a href="#" id="to_top">
									Наверх
								</a>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</footer>


		<div class="modals">
			<div id="modal1" class="modal">
				<div class="modal-content">
